pipeline {
    agent any

    stages {
        stage('clone') {
            steps {
                git branch: 'main',url:'https://github.com/pavan-deshagani/Calculator.git'
            }
        }
        
        stage('compile') {
            steps {
                sh 'javac calculator.java'
            }
        }
        
        stage('build') {
            steps {
                sh 'java calculator 2 1'
            }
        }
        stage('Deploy') {
            steps {
                echo 'deployment is completed'
            }
        }
        
    }
    

        
    
    
    
}
